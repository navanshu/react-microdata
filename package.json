{
  "name": "react-microdata",
  "version": "0.1.0",
  "scripts": {
    "tsc": "tsc",
    "@react-microdata/gen": "./node_modules/@react-microdata/gen/bin/schema-gen.js",
    "gen": "npm run @react-microdata/gen -- --file=./shema.json --dest=./microdata",
    "deploy": "npm publish --workspaces",
    "build": "npm run build --workspaces"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@react-microdata/gen": "*",
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "ts-loader": "^9.3.1"
  },
  "peerDependencies": {
    "react": ">= 15.0.0",
    "react-dom": ">= 15.0.0"
  },
  "keywords": [
    "react",
    "schema",
    "microdata",
    "schema.org"
  ],
  "workspaces": [
    "./microdata/*",
    "./packages/*"
  ],
  "license": "Apache-2.0",
  "private": false,
  "homepage": "https://github.com/rilexus/react-microdata",
